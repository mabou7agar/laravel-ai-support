# Laravel AI Engine Configuration

#--------------------------------------------------------------------------
# Debug & Logging
#--------------------------------------------------------------------------
AI_ENGINE_DEBUG=false

#--------------------------------------------------------------------------
# Default Engine & Model
#--------------------------------------------------------------------------
AI_ENGINE_DEFAULT=openai
AI_DEFAULT_MODEL=gpt-4o-mini

# Dynamic Model Registry
# Models are stored in database and auto-synced
# Run: php artisan ai-engine:sync-models
AI_MODEL_AUTO_SYNC=true
AI_MODEL_CACHE_TTL=86400  # 24 hours

#--------------------------------------------------------------------------
# Intelligent RAG Configuration
#--------------------------------------------------------------------------
INTELLIGENT_RAG_ENABLED=true

# Auto-discovery: Leave empty to auto-discover all Vectorizable/RAGgable models
# Or specify manually: "App\Models\Document,App\Models\Post,App\Models\Email"
INTELLIGENT_RAG_COLLECTIONS=

# Auto-discovery settings
INTELLIGENT_RAG_AUTO_DISCOVER=true
INTELLIGENT_RAG_DISCOVERY_CACHE=3600

# RAG behavior
INTELLIGENT_RAG_MAX_CONTEXT=5
INTELLIGENT_RAG_MIN_SCORE=0.7
INTELLIGENT_RAG_ANALYSIS_MODEL=gpt-4o-mini
INTELLIGENT_RAG_INCLUDE_SOURCES=true

#--------------------------------------------------------------------------
# Rate Limiting
#--------------------------------------------------------------------------
AI_ENGINE_RATE_LIMIT_ENABLED=true
AI_ENGINE_RATE_LIMIT_PER_MINUTE=60
AI_ENGINE_RATE_LIMIT_PER_HOUR=1000

#--------------------------------------------------------------------------
# Memory Configuration
#--------------------------------------------------------------------------
AI_MEMORY_ENABLED=true
AI_MEMORY_DRIVER=database
AI_MEMORY_MAX_MESSAGES=50

# Memory Optimization
AI_MEMORY_OPTIMIZATION_ENABLED=true
AI_MEMORY_WINDOW_SIZE=10
AI_MEMORY_SUMMARY_THRESHOLD=20
AI_MEMORY_CACHE_TTL=300

# Database Memory Driver
AI_MEMORY_DB_CONNECTION=null
AI_MEMORY_DB_MAX_MESSAGES=100

# Redis Memory Driver
AI_MEMORY_REDIS_CONNECTION=default
AI_MEMORY_REDIS_PREFIX=ai_engine:
AI_MEMORY_REDIS_MAX_MESSAGES=100

# File Memory Driver
AI_MEMORY_FILE_PATH=storage/ai-engine/conversations
AI_MEMORY_FILE_MAX_MESSAGES=100

# MongoDB Memory Driver
AI_MEMORY_MONGODB_CONNECTION=mongodb://localhost:27017
AI_MEMORY_MONGODB_DATABASE=ai_engine
AI_MEMORY_MONGODB_MAX_MESSAGES=1000

#--------------------------------------------------------------------------
# Demo Routes
#--------------------------------------------------------------------------
AI_ENGINE_ENABLE_DEMO_ROUTES=true
AI_ENGINE_DEMO_PREFIX=ai-demo

#--------------------------------------------------------------------------
# OpenAI Configuration
#--------------------------------------------------------------------------
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_ORGANIZATION=
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_TIMEOUT=30
OPENAI_MODEL=gpt-4o-mini

