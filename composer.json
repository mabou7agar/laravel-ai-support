{
    "name": "m-tech-stack/laravel-ai-engine",
    "description": "A comprehensive Laravel package for multi-AI engine integration with credit management, streaming, and advanced features",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "ai",
        "openai",
        "anthropic",
        "gemini",
        "stable-diffusion",
        "multi-engine",
        "streaming",
        "credits"
    ],
    "authors": [
        {
            "name": "Mohamed Abou Hagar",
            "email": "m.abou7agar@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/cache": "^10.0|^11.0",
        "illuminate/queue": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0",
        "openai-php/client": "^0.8",
        "symfony/http-client": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "LaravelAIEngine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelAIEngine\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelAIEngine\\AIEngineServiceProvider"
            ],
            "aliases": {
                "AIEngine": "LaravelAIEngine\\Facades\\AIEngine"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
