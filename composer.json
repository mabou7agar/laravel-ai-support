{
    "name": "m-tech-stack/laravel-ai-engine",
    "description": "Laravel AI Engine with RAG (Retrieval-Augmented Generation), clickable option cards, source citations, and embeddable chat widget. Supports OpenAI, Anthropic, Google Gemini, and more.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel-9",
        "laravel-10",
        "laravel-11",
        "laravel-12",
        "ai",
        "rag",
        "retrieval-augmented-generation",
        "vector-search",
        "openai",
        "anthropic",
        "gemini",
        "gpt-4",
        "claude",
        "chat",
        "chatbot",
        "option-cards",
        "source-citations",
        "embeddable-widget",
        "blade-component",
        "streaming",
        "websocket",
        "real-time"
    ],
    "authors": [
        {
            "name": "Mohamed Abou Hagar",
            "email": "m.abou7agar@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/cache": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/queue": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "openai-php/client": "^0.8|^0.9|^0.10",
        "symfony/http-client": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0",
        "mockery/mockery": "^1.4"
    },
    "suggest": {
        "cboden/ratchet": "Required for WebSocket streaming support (^0.4)",
        "ext-mongodb": "Required for MongoDB memory driver support",
        "mongodb/mongodb": "Required for MongoDB memory driver support (^1.8)",
        "firebase/php-jwt": "Required for node JWT authentication (^6.0)",
        "tymon/jwt-auth": "Alternative JWT library for node authentication (^2.0)"
    },
    "autoload": {
        "psr-4": {
            "LaravelAIEngine\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelAIEngine\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelAIEngine\\AIEngineServiceProvider",
                "LaravelAIEngine\\LaravelAIEngineServiceProvider"
            ],
            "aliases": {
                "AIEngine": "LaravelAIEngine\\Facades\\AIEngine"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
